{
    "name": "kornineqnetwork/xevmium",
    "description": "A lightweight, elegant PHP framework for building modern websites with minimal setup",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "kornineqnetwork\\Xevmium\\": "src/"
        }
    },
    "authors": [
        {
            "name": "KornineQNetwork",
            "homepage": "https://github.com/KornineQNetwork"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "suggest": {
        "ext-gd": "For image processing capabilities"
    },
    "provide": {
        "kornineqnetwork/xevmium-implementation": "1.0.0"
    },
    "conflict": {
        "php": "<8.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "php",
        "website-builder",
        "framework",
        "themes",
        "responsive",
        "markdown",
        "xss-protection",
        "component-based",
        "lightweight",
        "static-site",
        "front-end",
        "ui",
        "templating",
        "modern",
        "html-generator"
    ],
    "support": {
        "issues": "https://github.com/KornineQNetwork/Xevmium/issues",
        "source": "https://github.com/KornineQNetwork/Xevmium",
        "docs": "https://github.com/KornineQNetwork/Xevmium/wiki"
    },
    "config": {
        "sort-packages": true
    }
}
